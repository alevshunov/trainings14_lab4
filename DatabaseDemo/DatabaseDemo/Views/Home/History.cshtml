@model System.Collections.Generic.IEnumerable<DatabaseDemo.Domain.Model.HistoryEntry>

@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/_Main.cshtml";
}

<h2>History</h2>

<table class="table table-striped">
    <tr>
        <th>#</th>
        <th>Date</th>
        <th>IP</th>
        <th>A</th>
        <th>B</th>
        <th>Operator</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Date</td>
            <td>@item.Ip</td>
            <td>@item.A</td>
            <td>@item.B</td>
            <td>@Html.Partial("Operator", item.Operator)</td>
            <td>@Html.ActionLink("Calculate", "Calculate", "Home", new { a = item.A, b = item.B, @operator = item.Operator }, null)</td>
        </tr>
    }
</table>